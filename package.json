{
  "name": "domodule",
  "version": "4.0.1",
  "description": "Class based modules",
  "main": "dist/domodule.js",
  "keywords": [
    "module",
    "browser"
  ],
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "prod": "scriptkit --env production",
    "test": "cat test/domodule.test.dist.js | tape-run --static . --browser phantomjs --render tap-spec",
    "prepublish": "scriptkit"
  },
  "scriptkit": {
    "files": {
      "dist/domodule.js": "lib/domodule.js",
      "test/domodule.test.dist.js": "test/domodule.test.js"
    },
    "scripts": {
      "babel": {
        "exclude": [ "node_modules/tape-rollup/*" ]
      }
    }
  },
  "watch": {
    "lib/*.js": [
      "lint",
      "test"
    ],
    "test/*.js": [
      "lint",
      "test"
    ]
  },
  "repository": "https://github.com/firstandthird/domodule",
  "author": "First+Third",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "eslint-config-firstandthird": "^3.0.2",
    "eslint-plugin-import": "^2.2.0",
    "phantomjs": "^2.1.7",
    "scriptkit": "0.0.20",
    "tap-spec": "^4.1.1",
    "tape-rollup": "4.6.4",
    "tape-run": "^2.1.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "dependencies": {
    "attrobj": "1.1.1",
    "domassist": "1.3.3"
  }
}
