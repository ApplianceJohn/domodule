{
  "name": "domodule",
  "version": "9.0.0",
  "description": "Class based modules",
  "keywords": [
    "module",
    "browser"
  ],
  "repository": "https://github.com/firstandthird/domodule",
  "license": "MIT",
  "author": "First+Third",
  "main": "lib/domodule.ts",
  "module": "dist/domodule.module.js",
  "types": "dist/domodule.d.ts",
  "publishConfig": {
    "source": "lib/domodule.ts",
    "main": "dist/domodule.js"
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "lint": "eslint ./lib ./test",
    "prepublishOnly": "npm run lint && microbundle",
    "test": "jest"
  },
  "dependencies": {
    "attrobj": "^3.1.0",
    "aug": "^4.0.0",
    "domassist": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "babel-jest": "^29.3.1",
    "eslint": "^7.32.0",
    "eslint-config-firstandthird": "^6.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "esmodule": "dist/domodule.modern.js",
  "watch": {
    "lib/*.js": [
      "lint",
      "test"
    ],
    "test/*.js": [
      "lint",
      "test"
    ]
  }
}
